{
	"name": "obsidian-note-icons",
	"version": "1.1.1",
	"description": "File and folder icons, colors, title colors, nav colors, and regex pattern matching for bulk styling in Obsidian.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"icons",
		"colors",
		"file-explorer",
		"patterns"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/parser": "^8.56.1",
		"esbuild": "0.25.5",
		"eslint": "^9.39.3",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"tslib": "2.4.0",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}
